local function run(msg, matches)
if not is_sudo(msg) then
 return nil
  end

 if msg.to.type == 'chat'then
 if matches[1] == "kick" then
 local chat = 'chat#'..msg.to.id
 local user = 'user#'..matches[2]
 chat_del_user(chat, user, callback, false)
 end

if matches[1] == "invite" then
local chat = 'chat#'..msg.to.id
local user = 'user#'..matches[2]
chat_add_user(chat, user, callback, false)
  end
end

if msg.to.type == 'channel'then

if matches[1] == "kick" then
 local Channel = 'channel#'..msg.to.id
 local user = 'user#'..matches[2]
 channel_kick_user(Channel, user, ok_cb, true)
 end

 if matches[1] == "invite" then
 local Channel = 'channel#'..msg.to.id
 local user = 'user#'..matches[2]
 channel_invite_user(Channel, user, ok_cb, true)
 end

end
end

return {
 description = "Kick and Invite SuperGroup",
 usage = {},
 patterns = {
 "^!(kick) (%d+)$",
 "^!(invite) (%d+)$"
 },
run = run
}

end
